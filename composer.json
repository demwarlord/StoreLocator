        "magenest/module-anz": "dev-master",
        "amasty/base": "^1.0",
        "amasty/module-special-promo-pro": "^2.0",
        "amasty/module-gift-card": "^1.1",
        "amasty/shopby": "^2.4",
        "amasty/quickview": "^1.0",
        "wyomind/core": "dev-master",
        "wyomind/cronscheduler": "dev-master"
    },
    "require-dev": {
        "PandaGroup/Salesforce": "dev-master",
        "salesforce-mc/fuel-sdk-php": "^1.1",
        "mirasvit/module-profiler": "^1.0",
        "PandaGroup/ShopByLook": "dev-master",
