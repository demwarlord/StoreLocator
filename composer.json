        "PandaGroup/Checkout": "dev-master",
        "PandaGroup/Careers": "dev-master",
        "PandaGroup/Spinner": "^1.0.0",
        "PandaGroup/Quickview": "dev-master",
        "PandaGroup/EmailTester": "dev-master",
        "PandaGroup/CronSchedulerExtender": "dev-master",
        "PandaGroup/GiftCardExtender": "dev-master",
        "PandaGroup/Westfield": "dev-master",
        "PandaGroup/SlackIntegration": "dev-master",
        "PandaGroup/MagentoSlackMonitor": "dev-master",
        "PandaGroup/RegionsPatcher": "dev-master",
        "PandaGroup/CouponExtender": "dev-master",
        "PandaGroup/SortByExtender": "dev-master",
