{
    "name": "magento/project-community-edition",
    "description": "eCommerce Platform for Growth (Community Edition)",
    "type": "project",
    "version": "2.1.5",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "magento/product-community-edition": "2.1.8",
        "PandaGroup/MigrationTool": "dev-master",
        "composer/composer": "@alpha",
        "strategery/magento2-infinitescroll": "^0.2.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "PandaGroup/Base": "dev-master",
        "PandaGroup/GuestWishlist": "dev-master",
        "PandaGroup/Migration": "dev-master",
        "PandaGroup/StoreLocator": "dev-master",
        "PandaGroup/Frontools": "dev-master",
        "PandaGroup/Checkout": "dev-master",
        "PandaGroup/Careers": "dev-master",
        "PandaGroup/Spinner": "^1.0.0",
        "PandaGroup/Quickview": "dev-master",
        "PandaGroup/ShopByLook": "dev-master",
        "PandaGroup/EmailTester": "dev-master",
        "PL/Migs": "dev-master",
        "amasty/base": "^1.0.5",
        "amasty/module-special-promo": "^1.2.8",
        "amasty/module-special-promo-pro": "^1.2.2",
        "amasty/module-gift-card": "^1.0.5",
        "amasty/shopby": "^1.15.2",
        "amasty/quickview": "^1.0.2"

    },
    "require-dev": {
        "phpunit/phpunit": "4.1.0",
        "squizlabs/php_codesniffer": "1.5.3",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.2.2",
        "fabpot/php-cs-fixer": "~1.2",
        "lusitanian/oauth": "~0.3 <=0.7.0",
        "sebastian/phpcpd": "2.0.0"
    },
    "config": {
        "use-include-path": true,
        "process-timeout": 1000
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/"
        },
        "psr-0": {
            "": "app/code/"
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        "1": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-migration"
        },
        "2": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-migration-tool"
        },
        "3": {
            "type": "vcs",
            "url": "https://github.com/pandagrouppl/storelocator"
        },
        "4": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-base-module.git"
        },
        "5": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-devtools.git"
        },
        "6": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-checkout.git"
        },
        "7": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-anz-payment.git"
        },
        "8": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-guest-wishlist.git"
        },
        "11": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-careers.git"
        },
        "12": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-spinner.git"
        },
        "13": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-shopbylook.git"
        },
        "14": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-email-tester.git"
        },
        "amastyGiftCard": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-amasty-giftcard.git"
        },
        "amastyQuickView": {
            "type": "vcs",
            "url": "git@bitbucket.org:l4w/magento2-quickview.git"
        },
        "amasty": {
            "type": "composer",
            "url": "https://composer.amasty.com/community/"
        }
    },
    "extra": {
        "magento-force": "override",
        "installer-types": [
            "magento2-panda-module",
            "magento2-panda-tool"
        ],
        "installer-paths": {
            "app/code/{$vendor}/{$name}/": [
                "type:magento2-panda-module"
            ]
        }
    }
}
