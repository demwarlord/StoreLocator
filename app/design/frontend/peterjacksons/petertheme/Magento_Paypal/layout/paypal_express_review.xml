<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="paypal.additional.actions">
            <block class="Amasty\GiftCard\Block\Checkout\Cart\GiftCard" name="paypal.cart.amgiftcard" as="amgiftcard" template="checkout/cart/giftcard.phtml"/>
        </referenceBlock>
        <referenceBlock name="paypal.cart.coupon">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">PandaGroup_Checkout::cart/coupon.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
